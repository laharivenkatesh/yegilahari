#include <stdio.h>
#define SIZE 5

struct Queue {
    int items[SIZE];
    int front;
    int rear;
};

void ENQUEUE(struct Queue *q, int value) {
    if (q->rear == SIZE - 1)
        printf("Queue is full\n");
    else {
        if (q->front == -1)
            q->front = 0;
        q->rear++;
        q->items[q->rear] = value;
    }
}

int DEQUEUE(struct Queue *q) {
    int item;
    if (q->front == -1)
        printf("Queue is empty\n");
    else {
        item = q->items[q->front];
        q->front++;
        if (q->front > q->rear)
            q->front = q->rear = -1;
        return item;
    }
}

void Display(struct Queue *q) {
    if (q->rear == -1)
        printf("Queue is empty\n");
    else {
        printf("Queue elements are:\n");
        for (int i = q->front; i <= q->rear; i++)
            printf("%d ", q->items[i]);
        printf("\n");
    }
}

int main() {
    struct Queue q;
    q.front = -1;
    q.rear = -1;

    ENQUEUE(&q, 1);
    ENQUEUE(&q, 2);
    ENQUEUE(&q, 3);

    Display(&q);

    DEQUEUE(&q);
    Display(&q);

    return 0;
}
